<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="login.html" />
            </files>
        </defaultDocument>
        <handlers>
            <add name="HTML Post" path="*.html, *.htm" verb="*" modules="WebDAVModule" resourceType="File" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
